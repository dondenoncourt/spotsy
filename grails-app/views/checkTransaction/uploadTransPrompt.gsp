<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
        <meta name="layout" content="internal" />
        <title>CheckTransaction Upload</title>
    </head>
    <body>
        <div class="body">
            <h1>CheckTransaction Upload</h1>
          <div>
            <g:form action="upload" method="post">
                    <g:datePicker name="transDate" value="${new Date()}" precision="day" years="${2009..2050}"/>
                    <input type="submit" class="buttons" name="upload" value="Upload"/>
            </g:form>
          </div>
        </div>
    </body>
</html>
